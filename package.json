{
  "name": "campuscore",
  "version": "1.0.0",
  "description": "Enterprise Training Backend for API Automation",
  "main": "src/server.js",
  "bin": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "node --watch src/server.js",
    "build": "pkg . --out-path build/CampusCore_API --compress GZip"
  },
  "pkg": {
    "targets": [
      "node18-win-x64",
      "node18-linux-x64",
      "node18-macos-x64"
    ],
    "outputPath": "build/CampusCore_API"
  },
  "keywords": [
    "api",
    "training",
    "automation",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  }
}
